{
  "name": "survey-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "mocha tests/**/*.js --timeout 15000 --exit",
    "coverage": "nyc yarn test",
    "coverage:report": "nyc report --reporter=html"
  },
  "all": true,
  "nyc": {
    "include": [
      "controllers/**/*js"
    ],
    "check-coverage": true,
    "per-file": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80,
    "watermarks": {
      "lines": [
        40,
        80
      ],
      "functions": [
        40,
        80
      ],
      "branches": [
        49,
        80
      ],
      "statements": [
        40,
        80
      ]
    }
  },
  "author": "Ashab Hussan",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.6.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.7.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "mocha": "^10.1.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0"
  }
}
